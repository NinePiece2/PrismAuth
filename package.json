{
  "name": "prismauth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss,html,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss,html,mdx}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --config ./prisma/prisma.config.ts",
    "db:studio": "prisma studio --config ./prisma/prisma.config.ts",
    "db:migrate": "prisma migrate dev --config ./prisma/prisma.config.ts",
    "db:seed": "bun prisma/seed.ts",
    "create:admin": "bun scripts/create-admin.ts",
    "generate:keys": "bun scripts/generate-keys.ts",
    "quickstart": "powershell -ExecutionPolicy Bypass -File ./scripts/quickstart.ps1"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.25",
    "ioredis": "^5.4.1",
    "iron-session": "^8.0.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.555.0",
    "nanoid": "^5.0.9",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "otpauth": "^9.4.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
