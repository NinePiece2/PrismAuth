    "@types/bun": "^1.3.3",
    "@types/node": "^24",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "prettier": "3.7.4",
    "prisma": "^7.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
