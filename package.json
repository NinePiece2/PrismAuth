{
  "name": "prismauth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss,html,mdx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss,html,mdx}\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev --config ./prisma/prisma.config.ts",
    "db:seed": "bun prisma/seed.ts",
    "generate:keys": "bun scripts/generate-keys.ts",
    "quickstart": "powershell -ExecutionPolicy Bypass -File ./scripts/quickstart.ps1"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "ioredis": "^5.4.1",
    "iron-session": "^8.0.3",
    "jose": "^5.9.6",
    "nanoid": "^5.0.9",
    "next": "16.0.3",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^10.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "prettier": "3.6.2",
    "prisma": "^7.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
